{
  "name": "__MSG_extName__",
  "description" : "Easy download photos, images from www.instagram.com",
  "version": "0.1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["*://*.instagram.com/*"],
      "js": ["content/index.js"]
    }
  ],
  "background": {
    "persistent": true,
    "scripts": ["background/index.js"]
  },
  "page_action": {
    "default_title": "__MSG_extName__",
    "default_popup": "popup/index.html",
    "default_icon": {
      "38": "$assets/webpack-38.png"
    }
  },
  "permissions": [
    "https://www.google-analytics.com/*",
    "contextMenus",
    "tabs",
    "background",
    "downloads",
    "storage"
  ],
  "icons": {
    "19": "icons/webpack-19.png",
    "38": "icons/webpack-38.png",
    "16": "icons/webpack-16.png",
    "32": "icons/webpack-32.png",
    "128": "icons/webpack-96.png"
  },
  "web_accessible_resources" : ["shared/google-analytics-bundle.js"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
