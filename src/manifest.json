{
  "name": "Instagram Web Tools",
  "description" : "Easy download photos, images from www.instagram.com",
  "version": "0.1.0",
  "manifest_version": 2,
  "homepage_url": "https://github.com/schovi/webpack-chrome-extension",
  "content_scripts": [
    {
      "matches": ["*://*.instagram.com/*"],
      "js": ["content/index.js"]
    }
  ],
  "background": {
    "persistent": false,
    "scripts": ["background/index.js"]
  },
  "page_action": {
    "default_title": "Ready for download",
    "default_icon": {
      "19": "$assets/webpack-19.png",
      "38": "$assets/webpack-38.png"
    }
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "background",
    "tabs",
    "downloads",
    "declarativeContent"
  ],
  "icons": {
    "19": "icons/webpack-19.png",
    "38": "icons/webpack-38.png",
    "16": "icons/webpack-16.png",
    "32": "icons/webpack-32.png",
    "128": "icons/webpack-96.png"
  }
}
